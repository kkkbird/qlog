{
  "description": "Pipeline for parsing the qlog udp logs",
  "processors": [
    {
      "json": {
        "field": "message",
        "target_field": "qlog"
      }
    },
    {
      "remove": {
        "field": "message"
      }
    },
    {
      "rename": {
        "field": "@timestamp",
        "target_field": "read_timestamp"
      }
    },
    {
      "date": {
        "field": "qlog.time",
        "target_field": "@timestamp",
        "formats": ["ISO8601"],
        "timezone": "Asia/Shanghai"
      }
    },
    {
      "remove": {
        "field": "qlog.time"
      }
    },
    {
      "rename": {
        "field": "qlog.file",
        "target_field": "filewithline"
      }
    },
    {
      "grok": {
        "field": "filewithline",
        "patterns": [
          "%{WORD:qlog.file}:%{INT:qlog.line}"
        ]        
      }
    },
    {
      "remove": {
        "field": "filewithline"
      }
    }
  ],
  "on_failure": [
    {
      "set": {
        "field": "error.message",
        "value": "{{ _ingest.on_failure_message }}"
      }
    }
  ]
}